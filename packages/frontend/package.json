{
  "name": "@x402arcade/frontend",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "sideEffects": [
    "*.css",
    "*.scss",
    "src/index.css",
    "src/styles/**/*.css"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:analyze": "ANALYZE=true vite build",
    "build:check": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:changed": "vitest --changed",
    "test:coverage": "vitest run --coverage",
    "test:ci": "vitest run --coverage --bail=1 --reporter=default --reporter=json --outputFile=./test-reports/json/results.json",
    "test:ui": "vitest --ui",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "storybook:test": "test-storybook",
    "storybook:test:ci": "test-storybook --ci --browsers chromium",
    "storybook:test:coverage": "test-storybook --coverage",
    "optimize:images": "tsx scripts/optimize-images.ts",
    "images:webp": "tsx scripts/optimize-images.ts --webp true",
    "images:all": "tsx scripts/optimize-images.ts --webp true --avif true"
  },
  "dependencies": {
    "@react-three/drei": "^9.122.0",
    "@react-three/fiber": "^8.18.0",
    "@react-three/postprocessing": "^2.19.1",
    "@tanstack/react-query": "^5.17.0",
    "clsx": "^2.1.0",
    "eslint": "^9.39.2",
    "framer-motion": "^10.17.4",
    "howler": "^2.2.4",
    "lucide-react": "^0.303.0",
    "postprocessing": "^6.35.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.21.1",
    "tailwind-merge": "^2.2.0",
    "three": "^0.160.1",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@storybook/addon-a11y": "^8.6.15",
    "@storybook/addon-docs": "^8.6.15",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-interactions": "^8.6.14",
    "@storybook/addon-themes": "^8.6.15",
    "@storybook/react": "^8.6.15",
    "@storybook/react-vite": "^8.6.15",
    "@storybook/test": "^8.6.14",
    "@storybook/test-runner": "^0.24.2",
    "@testing-library/jest-dom": "^6.2.0",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/howler": "^2.2.12",
    "@types/jest-axe": "^3.5.9",
    "@types/react": "^18.2.46",
    "@types/react-dom": "^18.2.18",
    "@types/three": "^0.182.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.2.0",
    "@vitest/ui": "^1.2.0",
    "autoprefixer": "^10.4.16",
    "axe-core": "^4.11.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.26",
    "globals": "^17.1.0",
    "jest-axe": "^10.0.0",
    "jsdom": "^23.2.0",
    "postcss": "^8.4.32",
    "rollup-plugin-visualizer": "^5.12.0",
    "storybook": "^8.6.15",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.3.3",
    "typescript-eslint": "^8.53.1",
    "viem": "^2.21.54",
    "vite": "^5.0.10",
    "vite-plugin-pwa": "^1.2.0",
    "vitest": "^1.2.0",
    "wagmi": "2.12.26"
  }
}
